<!-- Fixed Performance monitoring -->
<script>
// Fixed performance tracking
window.addEventListener('load', () => {
    // Use Performance API if available
    if (window.performance && window.performance.timing) {
        const perfData = window.performance.timing;
        const loadTime = perfData.loadEventEnd - perfData.navigationStart;
        
        // Only log if we have valid timing data
        if (loadTime > 0 && loadTime < 60000) { // Less than 60 seconds
            console.log(`Page load time: ${loadTime}ms`);
        }
    }
    
    // Alternative method using Performance Observer
    if (window.PerformanceObserver) {
        const observer = new PerformanceObserver((list) => {
            for (const entry of list.getEntries()) {
                if (entry.entryType === 'navigation') {
                    console.log(`Navigation timing: ${entry.duration.toFixed(2)}ms`);
                }
            }
        });
        observer.observe({ entryTypes: ['navigation'] });
    }
});

// Debug logging for settings
document.addEventListener('DOMContentLoaded', () => {
    console.log('DOM loaded, checking for app initialization...');
    
    // Check if app is initialized
    setTimeout(() => {
        if (window.app) {
            console.log('✅ App initialized successfully');
            console.log('Settings:', window.app.settings);
        } else {
            console.error('❌ App not initialized');
        }
        
        if (window.jobManager) {
            console.log('✅ Job manager initialized');
        } else {
            console.error('❌ Job manager not initialized');
        }
    }, 100);
});
</script>
